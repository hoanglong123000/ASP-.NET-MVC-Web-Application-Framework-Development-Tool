@using  Web.Student.Models
@using Service.Core.Components
@{
    var baseData = ViewData["BaseData"] as BaseData;
    var groups = baseData.FeatureGroups;

}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="google-site-verification" content="VlwHRdh-wzfUcWmmw11Xj9fj55dypOVJkPGTy7xKiok" />
    <title>@ViewBag.Title</title>

    @BundleHelper.Generate(new List<BundleType>()
    {
        BundleType.Web
    })

    <script>
        var roles = @Html.Raw(Json.Encode(groups));
    </script>

</head>
<body>
    <div id="webaby_plugin_include">
        @RenderSection("assets", required: false)
    </div>
    @Html.Partial("~/Views/"+ version +"/Web/Shared/header.cshtml", true)
<div class="page-container">
    <div class="page-content">
        @Html.Partial("~/Views/"+ version +"/Web/Shared/navbar.cshtml")
        <div class="content-wrapper">
            @RenderBody()
            @Html.Partial("~/Views/"+ version +"/Web/Shared/footer.cshtml")
        </div>
    </div>
</div>
</body>
</html>
