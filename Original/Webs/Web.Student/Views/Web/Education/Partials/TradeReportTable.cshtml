@using Service.Core.Components
@using Service.Utility.Components
@using Service.Utility.Variables
@using Service.Education.Executes.Clothesmn.TradeHistories

@model List<TradeReport>


@{
    var datefrom = ViewData["datefrom"] as string;
    var dateto = ViewData["dateto"] as string;
}
@if (Model.Count > 0)
{

<div>lấy dữ liệu từ ngày <b>@datefrom</b> dến ngày <b>@dateto</b></div>
<div class="table-responsive" style="max-height:400px"> 
<table class="table table-sm table-base" >
    <thead>
        <tr>
            <th rowspan="2">
                Sản phẩm
            </th>
            <th rowspan="2">
                Tồn đầu kỳ
            </th>
            <th colspan="2" style="text-align: center;">Trong kỳ</th>
            <th rowspan="2">Tồn cuối kỳ</th>
 
        </tr>
        <tr>
            <th style="text-align: center;">
                Nhập
            </th>
            <th style="text-align: center;">
                Xuất
            </th>
        </tr>

    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ObjClothes.Name</td>
                <td>@item.TDK</td>
                <td>@item.AmountImported</td>
                <td>@item.AmountExported</td>
                <td>@item.TCK</td>
            </tr>
        }

        </tbody>
    </table>
    </div>}
else
{ 
    <div>No data</div>

}
<script>
    var model = @Html.Raw(Json.Encode(Model));
    console.log('text du lieu: ', model);

    
</script>